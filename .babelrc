{
  "presets": ["next/babel", "@zeit/next-typescript/babel"],
  "plugins": [
    ["relay", { "artifactDirectory": "./generated" }],
    // https://github.com/zeit/next.js/issues/5989
    [
      "module-resolver",
      {
        "alias": {
          "^react-native$": "react-native-web"
        }
      }
    ]
  ]
}
