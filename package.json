{
  "name": "este",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "10.x.x"
  },
  "scripts": {
    "prisma": "prisma",
    "ts-node": "node -r esm -r dotenv/config -r ts-node/register/transpile-only",
    "dev:api": "nodemon --watch 'api' --exec 'npm run ts-node api/index.ts'",
    "dev:web": "next",
    "dev:playground": "prisma playground --port 5000 --server-only",
    "dev:relay": "npm run gen:relay -- --watch",
    "dev": "run-p dev:api dev:web dev:playground dev:relay",
    "build": "next build",
    "now-build": "next build",
    "start:api": "cross-env NODE_ENV=production npm run ts-node api/index.ts",
    "start:web": "next start",
    "start": "run-p start:api start:web",
    "test:lint": "tslint -p .",
    "test:tsc": "tsc --project tsconfig.json",
    "test:build": "npm run build",
    "test": "run-s gen:relay:ci test:lint test:tsc test:build",
    "deps": "rimraf 'package-lock.json' 'node_modules' && npm i",
    "docker:up": "cd prisma && docker-compose up -d && cd ../",
    "docker:down": "cd prisma && docker-compose down && cd ../",
    "gen:api": "cd api && graphqlgen",
    "gen:relay": "relay-compiler --src . --include 'components/**' 'pages/**' --schema 'api/schema.graphql' --language typescript --artifactDirectory 'generated'",
    "gen:relay:ci": "npm run gen:relay -- --watchman false",
    "gen": "run-s gen:api gen:relay",
    "env": "npm run ts-node scripts/env",
    "analyze": "cross-env BUNDLE_ANALYZE=both next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "fix:lint": "tslint -p . --fix",
    "prisma:deploy": "prisma deploy --env-file .env",
    "prisma:delete": "prisma delete --env-file .env",
    "deploy": "npm run ts-node scripts/deploy.ts"
  },
  "dependencies": {
    "@playlyfe/gql": "^2.6.1",
    "@sentry/browser": "^4.5.3",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "acorn": "^6.0.5",
    "apollo-server": "^2.3.1",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.3.1",
    "esm": "^3.0.84",
    "graphql": "^14.0.2",
    "graphql-import": "^0.7.1",
    "gravatar": "^1.8.0",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.4.0",
    "next": "8.0.0-canary.14",
    "nprogress": "^0.2.0",
    "prisma-client-lib": "^1.22.2",
    "prop-types": "^15.6.2",
    "react": "16.8.0-alpha.0",
    "react-art": "16.8.0-alpha.0",
    "react-dom": "16.8.0-alpha.0",
    "react-intl": "^2.7.2",
    "react-native-web": "^0.9.8",
    "react-relay": "^1.7.0",
    "relay-runtime": "^1.7.0",
    "utility-types": "^3.2.0",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.3.2",
    "@types/dotenv": "^6.1.0",
    "@types/graphql": "^14.0.3",
    "@types/gravatar": "^1.4.28",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/next": "^7.0.5",
    "@types/node": "^10.12.15",
    "@types/nprogress": "0.0.29",
    "@types/react": "^16.7.11",
    "@types/react-dom": "^16.0.11",
    "@types/react-intl": "^2.3.14",
    "@types/react-native": "^0.55.28",
    "@types/react-relay": "^1.3.9",
    "@types/relay-runtime": "^1.3.6",
    "@types/styled-jsx": "^2.2.7",
    "@types/validator": "^10.9.0",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-module-resolver": "^3.1.2",
    "babel-plugin-relay": "^1.7.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "graphqlgen": "^0.5.0",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "prisma": "^1.23.0",
    "relay-compiler": "^1.7.0",
    "relay-compiler-language-typescript": "^1.1.1",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  }
}
