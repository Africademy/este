{
  "version": 2,
  "name": "este",
  "alias": "este.io",
  "builds": [
    {
      "src": "packages/api/index.ts",
      "use": "@now/node-server@canary"
    },
    {
      "src": "packages/web/package.json",
      "use": "@now/next@canary"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/packages/api/"
    },
    {
      "src": "/(.*)",
      "dest": "/packages/web/$1",
      "headers": {
        "x-request-path": "$1"
      }
    }
  ]
}
