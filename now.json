{
  "version": 2,
  "name": "este",
  "alias": "este.io",
  "env": {
    "IS_NOW": "true"
  },
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@now/node-server@canary"
    },
    {
      "src": "next.config.js",
      "use": "@now/next@canary"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/"
    }
  ]
}
